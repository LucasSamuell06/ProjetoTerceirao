<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cor7"
    tools:context=".NovoExercicio4Activity">

    <TextView
        android:id="@+id/TextoNovoExe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aldrich"
        android:text="Meu perfil"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082"
        tools:layout_editor_absoluteX="-16dp" />

    <TextView
        android:id="@+id/TextoNovoExe3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="Qual sua experiência praticando musculação?"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:fontFamily="@font/aldrich"
        android:textAlignment="center"
        android:textSize="22dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.16"
        tools:layout_editor_absoluteX="0dp" />

    <ImageButton
        android:id="@+id/BotaoExperiencia"
        android:layout_width="320dp"
        android:layout_height="85dp"
        android:background="@drawable/botao_personalizado2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.27"
        />

    <TextView
        android:id="@+id/textoExperiencia1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iniciante"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:fontFamily="@font/aldrich"
        app:layout_constraintBottom_toBottomOf="@+id/BotaoExperiencia"
        app:layout_constraintEnd_toEndOf="@+id/BotaoExperiencia"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="@+id/BotaoExperiencia"
        app:layout_constraintTop_toTopOf="@+id/BotaoExperiencia"
        app:layout_constraintVertical_bias="0.147" />

    <TextView
        android:id="@+id/textoIniciante"
        android:layout_width="290dp"
        android:layout_height="43dp"
        android:fontFamily="@font/aldrich"
        android:text="Com menos de 2 meses de pratica de exercicios."
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/BotaoExperiencia"
        app:layout_constraintEnd_toEndOf="@+id/BotaoExperiencia"
        app:layout_constraintStart_toStartOf="@+id/BotaoExperiencia"
        app:layout_constraintTop_toTopOf="@+id/BotaoExperiencia"
        app:layout_constraintVertical_bias="0.833" />

    <ImageButton
        android:id="@+id/BotaoExperiencia1"
        android:layout_width="320dp"
        android:layout_height="85dp"
        android:background="@drawable/botao_personalizado2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.616" />

    <TextView
        android:id="@+id/textoExperiencia3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Avançado"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:fontFamily="@font/aldrich"
        app:layout_constraintBottom_toBottomOf="@+id/BotaoExperiencia1"
        app:layout_constraintEnd_toEndOf="@+id/BotaoExperiencia1"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="@+id/BotaoExperiencia1"
        app:layout_constraintTop_toTopOf="@+id/BotaoExperiencia1"
        app:layout_constraintVertical_bias="0.147" />

    <TextView
        android:id="@+id/textoIntermediario"
        android:layout_width="290dp"
        android:layout_height="43dp"
        android:fontFamily="@font/aldrich"
        android:text="Com pelo menos 1 ano de prática."
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/BotaoExperiencia1"
        app:layout_constraintEnd_toEndOf="@+id/BotaoExperiencia1"
        app:layout_constraintStart_toStartOf="@+id/BotaoExperiencia1"
        app:layout_constraintTop_toTopOf="@+id/BotaoExperiencia1"
        app:layout_constraintVertical_bias="0.833" />

    <ImageButton
        android:id="@+id/BotaoExperiencia2"
        android:layout_width="320dp"
        android:layout_height="85dp"
        android:background="@drawable/botao_personalizado2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.436" />

    <TextView
        android:id="@+id/textoExperiencia2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Intermediário"
        android:textSize="18dp"
        android:textColor="@color/white"
        android:fontFamily="@font/aldrich"
        app:layout_constraintBottom_toBottomOf="@+id/BotaoExperiencia2"
        app:layout_constraintEnd_toEndOf="@+id/BotaoExperiencia2"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="@+id/BotaoExperiencia2"
        app:layout_constraintTop_toTopOf="@+id/BotaoExperiencia2"
        app:layout_constraintVertical_bias="0.147" />

    <TextView
        android:id="@+id/textoAvancado"
        android:layout_width="290dp"
        android:layout_height="43dp"
        android:fontFamily="@font/aldrich"
        android:text="De 2 à 6 meses de prática."
        android:textColor="@color/white"
        android:textSize="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/BotaoExperiencia2"
        app:layout_constraintEnd_toEndOf="@+id/BotaoExperiencia2"
        app:layout_constraintStart_toStartOf="@+id/BotaoExperiencia2"
        app:layout_constraintTop_toTopOf="@+id/BotaoExperiencia2"
        app:layout_constraintVertical_bias="0.833" />

    <Button
        android:id="@+id/botaoNivel"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:onClick="next2"
        android:text="continuar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.844" />

</androidx.constraintlayout.widget.ConstraintLayout>